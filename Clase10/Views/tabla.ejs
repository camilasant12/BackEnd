<% if(productos.length > 0){ %>
    <div class="table-responsive">
        <table class="table table-dark">
            <tr style="color: yellow;"> <th>Nombre</th> <th>Precio</th> <th>Url</th> </tr>
            <%productos.forEach(function(persona) {%>
            <tr>
                <td><%=persona.nombre%></td>
                <td><%=persona.precio%></td>
                <td><img src="<%=persona.foto%>"></td>
            </tr>
            <%});%>
        </table>
    </div>
<% } else{ %>
    <h3 class="alert alert-danger">no se encontraron datos</h1>
<% } %>